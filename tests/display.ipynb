    "from itertools import zip_longest\n",
    "import json\n",
    "import heapq\n",
    "from rich.console import Console\n",
    "from rich.table import Table\n",
    "\n",
    "iter = '01'\n",
    "game = '0001'\n",
    "file_path = '../'\n",
    "file_path += 'outputs/2024-08-27/17-52-51'\n",
    "\n",
    "player0_path = file_path + f'/iteration_{iter}/player_0_iter_{iter}_game_{game}.json'\n",
    "player1_path = file_path + f'/iteration_{iter}/player_1_iter_{iter}_game_{game}.json'\n",
    "\n",
    "table_name = 'agreement_example'\n",
    "table_name += '.html'\n",
    "to_html = True\n",
    "\n",
    "\n",
    "with open(player0_path, 'r') as f:\n",
    "    convo_player_0 = json.load(f)\n",
    "\n",
    "with open(player1_path, 'r') as f:\n",
    "    convo_player_1 = json.load(f)\n",
    "\n",
    "rules_prompt = convo_player_0[0]['content']\n",
    "\n",
    "def is_moderator(item):\n",
    "    return item['role'] == 'user'\n",
    "\n",
    "def filter_elements(lst):\n",
    "    lst = lst[1:]\n",
    "    last_element = lst[-1]\n",
    "    result = []\n",
    "    for id, item in enumerate(lst):\n",
    "        if is_moderator(item):\n",
    "            result.append(lst[id-1]['content'])\n",
    "    \n",
    "    result.append(last_element['content'])\n",
    "    return result\n",
    "\n",
    "def extract_content(tag, text):\n",
    "    start_tag = f\"<{tag}>\"\n",
    "    end_tag = f\"</{tag}>\"\n",
    "    if start_tag not in text:\n",
    "        return \"\"\n",
    "    start_index = text.find(start_tag) + len(start_tag)\n",
    "    end_index = text.find(end_tag)\n",
    "    return text[start_index:end_index].strip()\n",
    "\n",
    "# Filtered lists\n",
    "output_player_0 = filter_elements(convo_player_0)\n",
    "output_player_1 = filter_elements(convo_player_1)\n",
    "\n",
    "# Combine the lists\n",
    "convo = [item for pair in zip_longest(output_player_0, output_player_1) for item in pair if item is not None]\n",
    "\n",
    "# Initialize the console\n",
    "console = Console(record=to_html)\n",
    "\n",
    "# Create a table to display the conversation\n",
    "table = Table(show_header=True, header_style=\"bold magenta\")\n",
    "table.add_column(\"Role\", style=\"dim\", width=12)\n",
    "table.add_column(\"Content\" )\n",
    "\n",
    "\n",
    "table.add_row('Moderator', rules_prompt, style = \"green\")\n",
    "table.add_row(\"\", \"\")\n",
    "for id, content in enumerate(convo):\n",
    "    reasoning = extract_content(\"reason\", content)\n",
    "    message = extract_content(\"message\", content) or extract_content(\"finalize\", content)\n",
    "    if id > 0:\n",
    "        table.add_row('Moderator', \"(prompt)\", style = \"dim green\")\n",
    "        table.add_row(\"\", \"\")\n",
    "    if id % 2 == 0:\n",
    "      table.add_row('Player 0', reasoning, style = \"dim blue\")\n",
    "      table.add_row(\"\", \"\")\n",
    "      table.add_row('', message, style = \"blue\")\n",
    "    else:\n",
    "      table.add_row('Player 1', reasoning, style = \"dim red\")\n",
    "      table.add_row(\"\", \"\")\n",
    "      table.add_row('', message, style = \"red\")\n",
    "    table.add_row(\"\", \"\")\n",
    "table.add_section()\n",
    "\n",
    "console.print(table)\n",
    "\n",
    "if to_html:\n",
    "    html = console.export_html()\n",
    "\n",
    "    custom_css = \"\"\"\n",
    "    <style>\n",
    "        body {\n",
    "            font-size: 12px;\n",
    "        }\n",
    "    </style>\n",
    "    \"\"\"\n",
    "\n",
    "    html = html.replace(\"<head>\", f\"<head>{custom_css}\")\n",
    "\n",
    "    with open(table_name, \"w\") as f:\n",
    "        f.write(html)\n"
   ]
